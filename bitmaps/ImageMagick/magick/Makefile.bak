all:: libMagick.a

libMagick.a: $(OBJS)
	$(RM) $@
	$(AR) $@ $(OBJS)
	$(RANLIB) $@

install:: libMagick.a
	@if [ -d $(DESTDIR)$(LOCALLIB) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LOCALLIB)); fi
	$(INSTALL) -c $(INSTLIBFLAGS) libMagick.a $(DESTDIR)$(LOCALLIB)
	$(RANLIB) $(RANLIBINSTFLAGS) $(DESTDIR)$(LOCALLIB)/libMagick.a

install:: $(HEADERS)
	@if [ -d $(DESTDIR)$(LOCALINC) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LOCALINC)); fi
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(HEADERS); do \
	(set -x; $(INSTALL) -c $(INSTALLFLAGS) $$i $(DESTDIR)$(LOCALINC)); \
	done

depend::
	$(DEPEND) $(DEPENDFLAGS) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

