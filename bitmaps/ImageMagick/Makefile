# DIRENT_DEFINES= -DDIRENT=0

# Set DoSharedLib to 1 to build ImageMagick with shared
# libraries.
#
# #define DoSharedLib  1

# If you have the JPEG, TIFF, or XPM libraries define HasJPEG, HasTIFF, or
# HasXPM respectively.  Make sure the path names are correct.  See README
# for more details.
#
# #define HasJPEG
# #define HasTIFF
# #define HasXPM

# Might need extra libraries for xtp to link correctly.
#
#  EXTRA_LOAD_FLAGS= -lnsl

LOCALDIR= /usr/local/bin
LOCALINC= /usr/local/include
LOCALLIB= $(USRLIBDIR)

MAGICK_INCLUDES= -I$(TOP)/magick
MAGICK_LIBRARIES= -L$(TOP)/magick -lMagick

DEFINES= $(MAGICK_INCLUDES) $(DIRENT_DEFINES) $(JPEG_DEFINES) $(JPEG_INCLUDES)\
  $(TIFF_DEFINES) $(TIFF_INCLUDES) $(XPM_DEFINES) $(XPM_INCLUDES)
LOCAL_LIBRARIES= $(MAGICK_LIBRARIES) $(JPEG_LIBRARIES) $(TIFF_LIBRARIES) \
  $(XPM_LIBRARIES)
REQUIRED_LIBRARIES= $(JPEG_LIBRARIES) $(TIFF_LIBRARIES) $(XPM_LIBRARIES)

HEADERS = magick/magick.h magick/image.h magick/compress.h magick/utility.h \
  magick/X.h magick/widget.h magick/PreRvIcccm.h

SRCS = display.c import.c montage.c animate.c convert.c mogrify.c combine.c \
  segment.c
OBJS = display.o import.o montage.o animate.o convert.o mogrify.o combine.o \
  segment.o

PROGRAMS= display import montage animate convert mogrify combine segment

SUBDIRS= magick

all::
	@case '${MFLAGS}' in *[ik]*) set +e;; esac; \
	for i in $(SUBDIRS) ;\
	do \
	(cd $$i ; echo "making" all "in $(CURRENT_DIR)/$$i..."; \
	$(MAKE) $(MFLAGS) CCOPTIONS='$(CCOPTIONS)' all); \
	done

depend::
	@case '${MFLAGS}' in *[ik]*) set +e;; esac; \
	for i in $(SUBDIRS) ;\
	do \
	(cd $$i ; echo "depending" "in $(CURRENT_DIR)/$$i..."; \
	$(MAKE) $(MFLAGS)  depend); \
	done

all:: $(PROGRAMS)

X.h: magick/X.h
	$(RM) $@
	$(LN) $? $@

display: display.o
	$(RM) $@
	$(CC) -o $@ display.o $(LDOPTIONS) $(LOCAL_LIBRARIES) $(LDLIBS) $(XLIB) -lm $(EXTRA_LOAD_FLAGS)

clean::
	$(RM) display

install:: display
	@if [ -d $(DESTDIR)$(LOCALDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(LOCALDIR)); fi
	$(INSTALL) -c $(INSTPGMFLAGS)  display $(DESTDIR)$(LOCALDIR)

install.man:: display.man
	@if [ -d $(DESTDIR)$(MANDIR) ]; then set +x; \
	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MANDIR)); fi
	$(INSTALL) -c $(INSTMANFLAGS) display.man $(DESTDIR)$(MANDIR)/display.$(MANSUFFIX)

